{
  "name": "tree-sitter-gitattributes",
  "version": "0.1.5",
  "license": "MIT",
  "description": "gitattributes grammar for tree-sitter",
  "repository": "tree-sitter-grammars/tree-sitter-gitattributes",
  "author": {
    "name": "ObserverOfTime",
    "email": "chronobserver@disroot.org",
    "url": "https://observeroftime.github.io/"
  },
  "funding": {
    "url": "https://github.com/sponsors/ObserverOfTime",
    "type": "github"
  },
  "keywords": [
    "tree-sitter",
    "parser",
    "lexer",
    "gitattributes"
  ],
  "scripts": {
    "build": "tree-sitter generate --no-bindings",
    "parse": "tree-sitter parse",
    "test": "tree-sitter test"
  },
  "main": "bindings/node",
  "dependencies": {
    "nan": "^2.18.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "~0.21.0"
  },
  "tree-sitter": [
    {
      "scope": "source.gitattributes",
      "highlights": "queries/highlights.scm",
      "file-types": [
        ".gitattributes",
        "gitattributes"
      ]
    }
  ]
}
